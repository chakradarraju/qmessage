{
  "name": "Quora messaging enhancer",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://www.quora.com/messages/thread/*"],
      "js": ["contentscript.js"],
      "css": ["smiley.css"]
    }
  ],
  "permissions": [
    "tabs", "http://www.quora.com/messages/thread/*"
  ]
}
